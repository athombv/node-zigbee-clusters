{
  "name": "zigbee-clusters",
  "version": "1.4.0",
  "description": "Zigbee Cluster Library for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter list",
    "lint": "eslint .",
    "serve": "concurrently \"serve build/docs/\" \"npm run jsdoc:watch\"",
    "build": "npm run jsdoc",
    "jsdoc": "cd docs; jsdoc -c jsdoc.json",
    "jsdoc:watch": "npm-watch jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/athombv/node-zigbee-clusters.git"
  },
  "keywords": [
    "zigbee",
    "cluster",
    "library"
  ],
  "author": "Athom B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/athombv/node-zigbee-clusters/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/athombv/node-zigbee-clusters#readme",
  "devDependencies": {
    "braintree-jsdoc-template": "^3.3.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-athom": "^2.1.0",
    "jsdoc": "^3.6.4",
    "mocha": "^7.2.0",
    "npm-watch": "^0.6.0",
    "serve": "^11.3.1"
  },
  "dependencies": {
    "@athombv/data-types": "^1.0.4",
    "debug": "^4.1.1"
  },
  "watch": {
    "jsdoc": {
      "patterns": [
        "lib"
      ],
      "extensions": [
        "js"
      ]
    }
  }
}
